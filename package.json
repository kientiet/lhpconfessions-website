{
  "name": "blog",
  "version": "1.0.0",
  "description": "LHP Confessions",
  "scripts": {
    "start": "nodemon --watch server/data/**/*.js --exec \"npm run update && ./node_modules/.bin/babel-node server/startup.js\"",
    "stop": "mongo admin --eval 'db.shutdownServer()' > /dev/null | rm -rf db",
    "update": "echo",
    "deploy": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.js && npm run update && npm run build-server && cross-env NODE_ENV=production node ./build/startup.js",
    "build-server": "cross-env NODE_ENV=production ./node_modules/.bin/babel ./server --out-dir ./build",
    "lint": "eslint --ignore-path .gitignore client server",
    "heroku-postbuild": "cross-env NODE_ENV=production webpack --config --display-entrypoints webpack.config.js && cross-env NODE_ENV=production ./node_modules/.bin/babel ./server --out-dir ./lib",
    "clean": "rm -rf build && mkdir build"
  },
  "pre-commit": "lint",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/lhpconfessions/v1"
  },
  "rules": {
    "import/no-unresolved": 0
  },
  "keywords": [
    "react",
    "graphql",
    "express",
    "es6",
    "es7",
    "jsx",
    "webpack",
    "babel",
    "postcss",
    "scaffolding",
    "fullstack"
  ],
  "author": "Duy P",
